{
  "deployments_with_requirements": [
    {
      "event": "ADDED",
      "requirements": [
        {
          "kind": "service-mount",
          "name": "rbacService",
          "variable": "url",
          "version": "1.0"
        },
        {
          "kind": "service-mount",
          "name": "userService",
          "variable": "url"
        }
      ],
      "deployment": {
        "name": "homepage",
        "namespace": "homepage",
        "labels": {
          "app": "homepage",
          "decMgmtRequires-a-kind": "service-mount",
          "decMgmtRequires-a-name": "userService",
          "decMgmtRequires-a-variable-name": "url",
          "decMgmtRequires-b-kind": "service-mount",
          "decMgmtRequires-b-name": "rbacService",
          "decMgmtRequires-b-variable-name": "url",
          "decMgmtRequires-b-version": "1.0"
        }
      }
    },
    {
      "event": "ADDED",
      "requirements": [
        {
          "kind": "secret",
          "name": "db",
          "secret": "mysql-secret"
        }
      ],
      "deployment": {
        "name": "user",
        "namespace": "user",
        "labels": {
          "app": "user",
          "decMgmtRequires-a-kind": "secret",
          "decMgmtRequires-a-name": "db",
          "decMgmtRequires-a-secret-ref": "mysql-secret"
        }
      }
    }
  ],
  "exposed_services": [
    {
      "event": "ADDED",
      "offers": "userService",
      "service": {
        "name": "user",
        "namespace": "user",
        "labels": {
          "app": "user",
          "decMgmtProvides": "userService"
        },
        "ports": [
          {
            "name": "8080",
            "port": 8080,
            "protocol": "TCP",
            "target_port": 8080
          }
        ]
      }
    },
    {
      "event": "ADDED",
      "offers": "rbacService",
      "service": {
        "name": "rbac",
        "namespace": "rbac",
        "labels": {
          "app": "rbac",
          "decMgmtProvides": "rbacService"
        },
        "ports": [
          {
            "name": "8080",
            "port": 8080,
            "protocol": "TCP",
            "target_port": 8090
          }
        ]
      }
    }
  ]
}
